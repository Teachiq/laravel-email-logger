{
    "name": "krisell/laravel-email-logger",
    "description": "Log sent emails to your database",
    "keywords": [
        "krisell",
        "laravel-email-logger"
    ],
    "homepage": "https://github.com/krisell/laravel-email-logger",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Martin Krisell",
            "email": "martin.krisell@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3",
        "illuminate/support": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Krisell\\LaravelEmailLogger\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Krisell\\LaravelEmailLogger\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Krisell\\LaravelEmailLogger\\LaravelEmailLoggerServiceProvider"
            ]
        }
    }
}
